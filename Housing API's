    "''' Public API: your findings '''\n",
    "#I have designed the syntax of my API findings to only display the information I used in my diagrams, per the professor's guidance. \n",
    "#this API describes the average cost of living in Los Angeles, California. \n",
    "url = \"https://cities-cost-of-living-and-average-prices-api.p.rapidapi.com/cost_of_living\"\n",
    "\n",
    "querystring = {\"country\": \"united-states\", \"city\": \"los-angeles-ca\"}\n",
    "headers = {\n",
    "    \"X-RapidAPI-Key\": \"25fec6cee5mshed7d662975e156ep114890jsnc8c9cc12eac2\",\n",
    "    \"X-RapidAPI-Host\": \"cities-cost-of-living-and-average-prices-api.p.rapidapi.com\",\n",
    "}\n",
    "\n",
    "response = requests.get(url, headers=headers, params=querystring)\n",
    "if response.status_code == 200:\n",
    "    api_data = response.json()\n",
    "    country_name = api_data.get('Country Name')\n",
    "    city_name = api_data.get('City Name')\n",
    "    cost_of_living_total = api_data.get('Cost of Living Month Total')\n",
    "    def get_price(category, item_name):\n",
    "        try:\n",
    "            return next(item['Value'] for item in api_data[category] if item['Cost'] == item_name)\n",
    "        except StopIteration:\n",
    "            return None\n",
    "\n",
    "    items_to_extract = [\n",
    "        ('Restaurants prices', 'Domestic Beer, 0.5 liter Draught', 'Beer'),\n",
    "        ('Markets prices', 'Loaf of Fresh White Bread, 0.5 kg', 'Bread'),\n",
    "        ('Markets prices', 'Local Cheese, 1 kg', 'Local Cheese'),\n",
    "        ('Markets prices', 'Banana, 1 kg', 'Banana'),\n",
    "        ('Restaurants prices', 'Cappuccino', 'Cappuccino'),\n",
    "        ('Restaurants prices', 'Coca-Cola, 0.33 liter Bottle', 'Coke'),\n",
    "        ('Markets prices', 'Chicken Breasts, Boneless and Skinless, 1 kg', 'Chicken'),\n",
    "        ('Markets prices', 'Beef Round or Equivalent Back Leg Red Meat, 1 kg', 'Beef'),\n",
    "        ('Markets prices', 'Milk, Regular,1 liter', 'Milk'),\n",
    "        ('Markets prices', 'Eggs, 12 pack', '12 Eggs'),\n",
    "        ('Rent Per Month prices', 'One bedroom apartment outside of city centre', 'Apartment Outside City Center')\n",
    "    \n",
    "    ]\n",
    "\n",
    "   \n",
    "    print(f'Country Name: {country_name}')\n",
    "    print(f'City Name: {city_name}')\n",
    "    print(f'Cost of Living Month Total: {cost_of_living_total}')\n",
    "\n",
    "    for category, item_name, label in items_to_extract:\n",
    "        price = get_price(category, item_name)\n",
    "        print(f'{label} Price: {price}')\n",
    "\n",
    "else:\n",
    "    print(f\"Error: {response.status_code}\")\n",
    "    \n",
    "\n",
